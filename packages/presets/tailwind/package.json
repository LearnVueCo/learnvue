{
  "name": "@learnvue/tailwind-preset",
  "type": "module",
  "version": "0.0.2",
  "description": "",
  "author": "Matt Maribojoc <matt@learnvue.co>",
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "after:bump": "yarn build"
  },
  "devDependencies": {
    "@types/node": "^18.14.1",
    "bumpp": "^9.0.0",
    "eslint": "^8.34.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.1.2",
    "pnpm": "^7.27.1",
    "release-it": "^15.9.0",
    "rimraf": "^4.1.2",
    "simple-git-hooks": "^2.8.1",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vite": "^4.1.4",
    "vitest": "^0.28.5"
  },
  "release-it": {
    "npm": {
      "access": "public"
    },
    "git": {
      "commitMessage": "chore(release)[presets/tailwind]: release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
